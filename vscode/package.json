{
  "publisher": "xbase-lab",
  "name": "xbase",
  "displayName": "XBase",
  "version": "0.2.0",
  "packageManager": "pnpm@7.0.1",
  "description": "An XCode replacement-ish development environment that aims to be your reliable XCode alternative to develop exciting new apple software products ðŸš€",
  "license": "MIT",
  "homepage": "https://github.com/xbase-lab/[name]#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/xbase-lab/[name]"
  },
  "bugs": {
    "url": "https://github.com/xbase-lab/[name]/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/init.js",
  "icon": "res/icon.png",
  "files": [
    "vscode/dist"
  ],
  "engines": {
    "vscode": "^1.68.0"
  },
  "activationEvents": [
    "onStartupFinished",
    "workspaceContains:Package.swift",
    "workspaceContains:Project.swift",
    "workspaceContains:project.yaml"
  ],
  "contributes": {
    "commands": [
      {
        "command": "xbase.run",
        "title": "XBase: Run"
      },
      {
        "command": "xbase.build",
        "title": "XBase: Build"
      },
      {
        "command": "xbase.watch",
        "title": "XBase: Watch"
      }
    ]
  },
  "scripts": {
    "build": "tsup",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "format": "prettier --check xbase test",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/ni": "^0.16.2",
    "@babel/types": "^7.18.4",
    "@types/node": "^17.0.41",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.3.0",
    "pnpm": "^7.1.9",
    "prettier": "2.5.1",
    "tsup": "^6.1.0",
    "typescript": "^4.7.3",
    "vite": "^2.9.11",
    "vsce": "^2.9.1"
  },
  "dependencies": {
    "iter-ops": "^1.6.2",
    "tiny-glob": "^0.2.9"
  }
}
